<%= semantic_form_for @datum, :html => { :class => 'form-horizontal' } do |f| %>
 
  
  <div class="control-group">
   
    <div class="controls">
       <!--method ds helper -->
    <%= f.input :sequencing_id, :as => :radio, :collection => filtre_elt_associe(@project_associe_id,"sequencing") %>
       </div>
  </div>
  
  
  <div class="control-group">
    <div class="controls">
      <%= f.input :data_file, :class => 'text_area' %>
    </div>
  </div>
 

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                data_path, :class => 'btn' %>
  </div>
<% end %>
