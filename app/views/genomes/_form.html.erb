<%= semantic_form_for @genome, :html => { :class => 'form-horizontal' } do |f| %>
 
  
  
  <div class="control-group">
    <%= f.label :genome_name, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :genome_name, :class => 'text_field'  %>
    </div>
  </div>
  
  <div class="control-group">
    <div class="controls">
        <!--method ds helper -->
    <%= f.input :data_assoc, :as => :radio, :collection => filtre_elt_associe(@project_associe_id,"data") %>
       </div>
  </div>
  
 
  <div class="control-group">
    <div class="controls">
      <%= f.input :genome_type, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
     <div class="controls">
      <%= f.input :genome_ref, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <div class="controls">
      <%= f.input :genome_params, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
     <div class="controls">
      <%= f.text_field :genome_annotateur, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <div class="controls">
      <%= f.input :genome_nbcontigs, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <div class="controls">
      <%= f.input :genome_nbscaffold, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <div class="controls">
      <%= f.input :genome_coverage, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <div class="controls">
      <%= f.input :genome_ncinquante, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <div class="controls">
      <%= f.input :genome_size, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                genomes_path, :class => 'btn' %>
  </div>
<% end %>
