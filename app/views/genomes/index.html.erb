<%- model_class = Genome.new.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:genome_name) %></th>
      <th><%= model_class.human_attribute_name(:data_id) %></th>
      <th><%= model_class.human_attribute_name(:genome_type) %></th>
      <th><%= model_class.human_attribute_name(:genome_ref) %></th>
      <th><%= model_class.human_attribute_name(:genome_params) %></th>
      <th><%= model_class.human_attribute_name(:genome_annotateur) %></th>
      <th><%= model_class.human_attribute_name(:genome_nbcontigs) %></th>
      <th><%= model_class.human_attribute_name(:genome_nbscaffold) %></th>
      <th><%= model_class.human_attribute_name(:genome_coverage) %></th>
      <th><%= model_class.human_attribute_name(:genome_ncinquante) %></th>
      <th><%= model_class.human_attribute_name(:genome_size) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @genomes.each do |genome| %>
      <tr>
        <td><%= link_to genome.id, genome_path(genome) %></td>
        <td><%= genome.genome_name %></td>
        <td><%= genome.data_id %></td>
        <td><%= genome.genome_type %></td>
        <td><%= genome.genome_ref %></td>
        <td><%= genome.genome_params %></td>
        <td><%= genome.genome_annotateur %></td>
        <td><%= genome.genome_nbcontigs %></td>
        <td><%= genome.genome_nbscaffold %></td>
        <td><%= genome.genome_coverage %></td>
        <td><%= genome.genome_ncinquante %></td>
        <td><%= genome.genome_size %></td>
        <td><%=l genome.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_genome_path(genome), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      genome_path(genome),
                      :method => :delete,
                      :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_genome_path,
            :class => 'btn btn-primary' %>
