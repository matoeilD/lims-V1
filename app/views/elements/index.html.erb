<h1>Listing elements</h1>




<table>
  <tr>

    <th>Experiment</th>
  
    <th>project</th>
    <th>users</th>
    <th>date</th>
  </tr>

<% @elements.each do |element| %>
  <tr>
    
    <td><%= element.experiments.collect { |a| a.exptype } %></td>
  
    <td><%= element.projects.collect { |a| a.projectname }%></td>
    <td><%= element.users.collect { |a| a.name } %></td>
    <td><%= element.created_at %></td>
    <td><%= link_to 'Show', element %></td>
    <td><%= link_to 'Edit', edit_element_path(element) %></td>
    <td><%= link_to 'Destroy', element, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Element', new_element_path %> 
