<%- model_class = @element.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

  <div class="alert alert-success"  id="notice">
    <button class="close" data-dismiss="alert">Ã—</button>
    <strong>Well done!</strong> <%= notice %>
    </div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:element_name) %>:</strong></dt>
  <dd><%= @element.element_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:element_type) %>:</strong></dt>
  <dd><%= @element.element_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:element_note) %>:</strong></dt>
  <dd><%= @element.element_note %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @element.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:element_id) %>:</strong></dt>
  <dd><%= @element.element_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:project_id) %>:</strong></dt>
  <dd><%= @element.project_id %></dd>
</dl>

<div class="form-actions">
  
  
  <%= link_to t('.back', :default => t("helpers.links.back")),
              cultures_path, :class => 'btn'  %>
              
              
              
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_element_path(@element), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              element_path(@element),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
