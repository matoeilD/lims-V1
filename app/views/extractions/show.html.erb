<%- model_class = @extraction.class -%>



<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:extraction_name) %>:</strong></dt>
  <dd><%= @extraction.extraction_name %></dd>
  
  <dt><strong><%= model_class.human_attribute_name(:date) %>:</strong></dt>
  <dd><%= display_fields_on_views(Element.find(@extraction.element_id).element_date) %></dd>
  
  <dt><strong><%= model_class.human_attribute_name(:projet_associÃ©) %>:</strong></dt>
  <dd><%= display_fields_on_views(Element.find(@extraction.element_id).projects.collect { |a| a.project_name  }.join(",")) %></dd>
  
   <dt><strong><%= model_class.human_attribute_name(:experimentator) %>:</strong></dt>
  <dd><%= display_fields_on_views(Element.find(@extraction.element_id).users.collect { |a| a.name  }.join(",")) %></dd>
  
  <dt><strong><%= model_class.human_attribute_name(:culture_associe) %>:</strong></dt>
  <dd><%= display_fields_on_views(@extraction.culture_assoc) %></dd>
  <dt><strong><%= model_class.human_attribute_name(:extraction_note) %>:</strong></dt>
  <dd><%= display_fields_on_views(@extraction.extraction_note) %></dd>
  
</dl>

<div class="form-actions">
  <%= link_to t('.back_to_index', :default => t("helpers.links.back_to_index")),
              elements_path, :class => 'btn'  %>
  <%= link_to t('.edit_further', :default => t("helpers.links.edit_further")),
              edit_extraction_path(@extraction), :class => 'btn' %>

</div>
