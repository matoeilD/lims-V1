<%- model_class = @extraction.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:extraction_name) %>:</strong></dt>
  <dd><%= @extraction.extraction_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:element_id) %>:</strong></dt>
  <dd><%= @extraction.element_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:culture_id) %>:</strong></dt>
  <dd><%= @extraction.culture_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:extraction_note) %>:</strong></dt>
  <dd><%= @extraction.extraction_note %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              extractions_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_extraction_path(@extraction), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              extraction_path(@extraction),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
