<%= semantic_form_for @extraction, :html => { :class => 'form-horizontal' } do |f| %>
  
    
    <div class="control-group">
    <%= f.label :extraction_name, :class => 'control-label' %>
    <div class="controls">
      <%= @extraction.extraction_name  %>
    </div>
  </div>
    
    <div class="control-group">
    <div class="controls">
      <!--method ds helper et ds extraction controller -->
    <%= f.input :culture_assoc,  :as => :radio, :collection => Culture.all.collect {|el| el.culture_name}
%>
    </div>
  </div>
    
  <div class="control-group">    
    <div class="controls">
      <%= f.input :extraction_note %>
       
    </div>
  </div>


  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
               elements_path, :class => 'btn' %>
  </div>
<% end %>


