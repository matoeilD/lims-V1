<%- model_class = @rnaseq.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= Element.find(@submission.element_id).element_name %></dd>
  
  <dt><strong><%= model_class.human_attribute_name(:date) %>:</strong></dt>
  <dd><%= Element.find(@culture.element_id).element_date %></dd>
  
  <dt><strong><%= model_class.human_attribute_name(:projet_associÃ©) %>:</strong></dt>
  <dd><%= Element.find(@submission.element_id).projects.collect { |a| a.projectname  }.join(",") %></dd>
  
   <dt><strong><%= model_class.human_attribute_name(:experimentator) %>:</strong></dt>
  <dd><%= Element.find(@submission.element_id).users.collect { |a| a.name  }.join(",") %></dd>
  
  <dt><strong><%= model_class.human_attribute_name(:rnaseq_name) %>:</strong></dt>
  <dd><%= @rnaseq.rnaseq_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:data_id) %>:</strong></dt>
  <dd><%= @rnaseq.data_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rnaseq_tools) %>:</strong></dt>
  <dd><%= @rnaseq.rnaseq_tools %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rnaseq_params) %>:</strong></dt>
  <dd><%= @rnaseq.rnaseq_params %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rnaseq_output) %>:</strong></dt>
  <dd><%= @rnaseq.rnaseq_output %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rnaseq_ref) %>:</strong></dt>
  <dd><%= @rnaseq.rnaseq_ref %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rnaseq_tablecount) %>:</strong></dt>
  <dd><%= @rnaseq.rnaseq_tablecount %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              rnaseqs_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_rnaseq_path(@rnaseq), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              rnaseq_path(@rnaseq),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
