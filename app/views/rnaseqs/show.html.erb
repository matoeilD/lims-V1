<%- model_class = @rnaseq.class -%>




<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
    <dt><strong><%= model_class.human_attribute_name(:rnaseq_name) %>:</strong></dt>
  <dd><%= @rnaseq.rnaseq_name %></dd>
  
  <dt><strong><%= model_class.human_attribute_name(:date) %>:</strong></dt>
  <dd><%= display_fields_on_views(Element.find(@rnaseq.element_id).element_date) %></dd>
  
  <dt><strong><%= model_class.human_attribute_name(:projet_associé) %>:</strong></dt>
  <dd><%=display_fields_on_views( Element.find(@rnaseq.element_id).projects.collect { |a| a.projectname  }.join(",")) %></dd>
  
   <dt><strong><%= model_class.human_attribute_name(:experimentator) %>:</strong></dt>
  <dd><%= display_fields_on_views(Element.find(@rnaseq.element_id).users.collect { |a| a.name  }.join(",")) %></dd>
  

  <dt><strong><%= model_class.human_attribute_name(:data_associé) %>:</strong></dt>
  <dd><%= display_fields_on_views(@rnaseq.data_assoc )%></dd>
  <dt><strong><%= model_class.human_attribute_name(:rnaseq_tools) %>:</strong></dt>
  <dd><%= display_fields_on_views(@rnaseq.rnaseq_tools) %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rnaseq_params) %>:</strong></dt>
  <dd><%= display_fields_on_views(@rnaseq.rnaseq_params) %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rnaseq_output) %>:</strong></dt>
  <dd><%= display_fields_on_views(@rnaseq.rnaseq_output )%></dd>
  <dt><strong><%= model_class.human_attribute_name(:rnaseq_ref) %>:</strong></dt>
  <dd><%= display_fields_on_views(@rnaseq.rnaseq_ref) %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rnaseq_tablecount) %>:</strong></dt>
  <dd><%= display_fields_on_views(@rnaseq.rnaseq_tablecount) %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              rnaseqs_path, :class => 'btn'  %>
  <%= link_to t('.edit_further', :default => t("helpers.links.edit_further")),
              edit_rnaseq_path(@rnaseq), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              rnaseq_path(@rnaseq),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
