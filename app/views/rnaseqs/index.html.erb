<%- model_class = Rnaseq.new.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:rnaseq_name) %></th>
      <th><%= model_class.human_attribute_name(:data_id) %></th>
      <th><%= model_class.human_attribute_name(:rnaseq_tools) %></th>
      <th><%= model_class.human_attribute_name(:rnaseq_params) %></th>
      <th><%= model_class.human_attribute_name(:rnaseq_output) %></th>
      <th><%= model_class.human_attribute_name(:rnaseq_ref) %></th>
      <th><%= model_class.human_attribute_name(:rnaseq_tablecount) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @rnaseqs.each do |rnaseq| %>
      <tr>
        <td><%= link_to rnaseq.id, rnaseq_path(rnaseq) %></td>
        <td><%= rnaseq.rnaseq_name %></td>
        <td><%= rnaseq.data_id %></td>
        <td><%= rnaseq.rnaseq_tools %></td>
        <td><%= rnaseq.rnaseq_params %></td>
        <td><%= rnaseq.rnaseq_output %></td>
        <td><%= rnaseq.rnaseq_ref %></td>
        <td><%= rnaseq.rnaseq_tablecount %></td>
        <td><%=l rnaseq.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_rnaseq_path(rnaseq), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      rnaseq_path(rnaseq),
                      :method => :delete,
                      :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_rnaseq_path,
            :class => 'btn btn-primary' %>
