<%= semantic_form_for @project do |f| %>
  <%= f.inputs do %>
    <%= f.input :projectname %>
    <%= f.input :projectdescription %>     
    <%= f.input :projecttype, :as => :radio,       :collection => ["genomique", "transcriptomique"]%>  
    <%= f.input :users, :label => "project header"%>
    <%= f.input :users, :label => "project members"%> 
    <%= f.input :elements %>
  <% end %>
    
    <%= f.actions :submit, :cancel %>
    

  <% end %>