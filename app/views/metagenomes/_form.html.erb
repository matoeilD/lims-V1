<%= semantic_form_for @metagenome, :html => { :class => 'form-horizontal' } do |f| %>
 
  
   <div class="control-group">
     
    <div class="controls">
        <!--method ds helper -->
    <%= f.input :data_id, :as => :radio, :collection => filtre_elt_associe(@project_associe_id,"data") %>
       </div>
  </div>

  <div class="control-group">
    <%= f.label :meta_tools, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :meta_tools, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :meta_params, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :meta_params, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :meta_output, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_area :meta_output, :class => 'text_area' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                metagenomes_path, :class => 'btn' %>
  </div>
<% end %>
