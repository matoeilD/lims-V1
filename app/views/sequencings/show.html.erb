<%- model_class = @sequencing.class -%>




    


<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
    <dt><strong><%= model_class.human_attribute_name(:sequencing_name) %>:</strong></dt>
  <dd><%= @sequencing.sequencing_name %></dd>
  
  <dt><strong><%= model_class.human_attribute_name(:date) %>:</strong></dt>
  <dd><%= display_fields_on_views(Element.find(@sequencing.element_id).element_date) %></dd>
  
  <dt><strong><%= model_class.human_attribute_name(:projet_associé) %>:</strong></dt>
  <dd><%=display_fields_on_views( Element.find(@sequencing.element_id).projects.collect { |a| a.project_name  }.join(",") )%></dd>
  
   <dt><strong><%= model_class.human_attribute_name(:experimentator) %>:</strong></dt>
  <dd><%=display_fields_on_views( Element.find(@sequencing.element_id).users.collect { |a| a.name  }.join(",") )%></dd>
  
  
 
  <dt><strong><%= model_class.human_attribute_name(:paired_end) %>:</strong></dt>
  <dd><%= display_fields_on_views(@sequencing.paired_end )%></dd>
  <dt><strong><%= model_class.human_attribute_name(:distance_cassure) %>:</strong></dt>
  <dd><%= display_fields_on_views(@sequencing.distance_cassure) %></dd>
 
  <dt><strong><%= model_class.human_attribute_name(:empcr_associé) %>:</strong></dt>
  <dd><%= display_fields_on_views(@sequencing.empcr_assoc) %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back_to_index', :default => t("helpers.links.back_to_index")),
              elements_path, :class => 'btn'  %>
  <%= link_to t('.edit_further', :default => t("helpers.links.edit_further")),
              edit_sequencing_path(@sequencing), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              sequencing_path(@sequencing),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
